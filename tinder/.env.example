# Copy this file to .env and fill in your credentials

# Required
TINDER_EMAIL=your_email@example.com
TINDER_PASSWORD=your_password

# Optional: google or facebook
LOGIN_METHOD=google

# Swiping behaviour
LIKES=100
RATIO=72.5%
SLEEP=1
RANDOMIZE_SLEEP=true

# Browser
HEADLESS=false
# PROXY format examples:
#   23.23.23.23:3128
#   user:password@23.23.23.23:3128
PROXY=

# Optional location spoofing
LATITUDE=
LONGITUDE=
